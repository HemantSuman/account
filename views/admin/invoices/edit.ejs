                    <label for="">Invoice No.</label>
                    <input name="invoice_no" value="<%= extraVar.results.my_model.invoice_no%>" type="text" class="form-control"  placeholder="">
                    <div class="invoice_no-error-message error-message"></div>
                  </div>
                </div>
                <div class="col-sm-2">
                  <div class="form-group">
                    <label for="">Date</label>
                    <input name="date" type="text" value="<%= extraVar.results.my_model.date%>" class="form-control date1"  placeholder="">
                    <div class="date-error-message error-message"></div>
                  </div>
                </div>
                <div class="col-sm-3">
                  <div class="form-group">
                    <label for="">Consignee Name</label>
                    <select name="consignee_no" class="form-control select2 consigneeAccount">
                      <option value="">Select Account</option>
                      <% extraVar.results.accounts.forEach(function(data){ %>
                        <option <%= data.id==extraVar.results.my_model.consignee_no ? 'selected':'' %> address="<%= data.address %>" gstin="<%= data.gstin %>" value="<%= data.id %>" title="AC00<%=data.id %>"><%= data.account_name %></option>
                      <% }); %>                  
                    </select>
                    <div class="consignee_no-error-message error-message"></div>
                  </div>
                </div>
                <div class="col-sm-3">
                  <div class="form-group">
                    <label for="">Buyer</label>
                    <select name="buyer" class="form-control select2">
                      <option value="">Select Account</option>
                      <% extraVar.results.accounts.forEach(function(data){ %>
                        <option <%= data.id==extraVar.results.my_model.buyer ? 'selected':'' %> gstin="<%= data.gstin %>" value="<%= data.id %>" title="AC00<%=data.id %>"><%= data.account_name %></option>
