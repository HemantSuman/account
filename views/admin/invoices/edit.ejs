                                      <div class="item_description_<%= index%>-error-message error-message"></div>
                                  </div>
                              </div>
                              <div class="col-sm-2">
                                  <div class="form-group">
                                      <small><b>No.: </b></small>
                                      <input readonly name="item[<%= index%>][no_of_pkg]" type="number" value="<%= item.no_of_pkg%>" class="form-control pkt item_no_of_pkg_<%= index%>" placeholder="">
                                      <div class="item_no_of_pkg_<%= index%>-error-message error-message"></div>
                                  </div>
                              </div>
                              <div class="col-sm-2">
                                  <div class="form-group">
                                      <small><b>Quantity: </b></small>
                                      <input readonly name="item[<%= index%>][quantity]" type="number" value="<%= item.quantity%>" class="form-control qty item_quantity_<%= index%>" placeholder="">
                                      <small><b>Stocks: </b><span  class="item_quantity_display_<%= index%>"></span></small>
                                      <div class="item_quantity_<%= index%>-error-message error-message"></div>
                                  </div>
                              </div>
                              <div class="col-sm-2">
                                  <div class="form-group">
                                      <small><b>Rate: </b></small>
                                      <input readonly name="item[<%= index%>][rate]" type="number" value="<%= item.rate%>" class="form-control rate item_rate_<%= index%>" placeholder="">
                                      <div class="item_rate_<%= index%>-error-message error-message"></div>
                                  </div>
                              </div>
                              <div class="col-sm-2">
                                  <div class="form-group">
                                      <small><b>GST (%): </b></small>
                                      <input readonly name="item[<%= index%>][gst]" type="number" value="<%= item.gst%>" class="form-control gst item_gst_<%= index%>" placeholder="">
                                      <div class="item_gst_<%= index%>-error-message error-message"></div>
