  $(document).on("change", ".selectSubItem", function () {

    let thisObj = $(this);
    let id = thisObj.val();
    let indexValue = (thisObj.attr('indexValue') == 0 ? '' : thisObj.attr('indexValue'));
    $(".item_quantity_"+indexValue).val('');
    $(".item_quantity_display_"+indexValue).text('');

    if(id){
      
      let type = $(".item_type_"+indexValue).val();
      $.ajax({
          url: "/admin/stocks/getBySubItemId",
          data: {id,type},
          //contentType: false,
          //processData: false,
          method: "POST",
          cache: false,
          success: function (response) {
            if(response){
              $(".item_quantity_"+indexValue).val("");
              $(".item_quantity_display_"+indexValue).text(response.quantity);
            }
          }, error: function (resData) {
              console.log(resData);
          }
      });
    } else {
      // $(".selectItem").trigger("change");
    }
  });

  function matchCustomConsigneeAccount(params, data) {
    // If there are no search terms, return all of the data
    if ($.trim(params.term) === '') {
      return data;
    }

    // Do not display the item if there is no 'text' property
    if (typeof data.text === 'undefined') {
      return null;
    }

    // `params.term` should be the term that is used for searching
    // `data.text` is the text that is displayed for the data object
    let tmp = data.text + " " + data.title;
    if (tmp.toUpperCase().indexOf(params.term.toUpperCase()) > -1) {
      var modifiedData = $.extend({}, data, true);
      // modifiedData.text += ' (matched)';

      // You can return modified objects from here
      // This includes matching the `children` how you want in nested data sets
      return modifiedData;
    }

    // Return `null` if the term should not be displayed
    return null;
  }

